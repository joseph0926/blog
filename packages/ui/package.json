    },
    "./hooks/*": {
      "types": "./hooks/*.ts",
      "default": "./hooks/*.ts"
    },
    "./styles/*": "./styles/*.css"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "joseph0926",
  "engines": {
    "node": ">=22.19.0"
  },
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3"
  }
}
